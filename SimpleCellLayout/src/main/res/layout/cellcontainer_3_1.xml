<?xml version="1.0" encoding="utf-8"?>

<com.jlnshen.widget.celllayout.SimpleCellLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:celllayout="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        celllayout:col="2"
        celllayout:row="2"
        android:id="@+id/celllayout">

    <FrameLayout
            android:id="@+id/simple_cell1"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            celllayout:cellX="0"
            celllayout:cellY="0"
            celllayout:colspan="2"
            />

    <FrameLayout
            android:id="@+id/simple_cell2"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            celllayout:cellX="0"
            celllayout:cellY="1"
            />

    <FrameLayout
            android:id="@+id/simple_cell3"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            celllayout:cellX="1"
            celllayout:cellY="1"
            />
</com.jlnshen.widget.celllayout.SimpleCellLayout>