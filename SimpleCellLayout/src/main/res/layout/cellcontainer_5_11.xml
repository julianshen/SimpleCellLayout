<?xml version="1.0" encoding="utf-8"?>

<com.jlnshen.widget.celllayout.SimpleCellLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:celllayout="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        celllayout:col="4"
        celllayout:row="6"
        android:id="@+id/celllayout">

    <FrameLayout
            android:id="@+id/simple_cell1"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            celllayout:cellX="0"
            celllayout:cellY="0"
            celllayout:rowspan="3"
            celllayout:colspan="2"
            />

    <FrameLayout
            android:id="@+id/simple_cell2"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            celllayout:cellX="2"
            celllayout:cellY="0"
            celllayout:rowspan="2"
            celllayout:colspan="2"
            />

    <FrameLayout
            android:id="@+id/simple_cell3"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            celllayout:cellX="0"
            celllayout:cellY="3"
            celllayout:colspan="2"
            celllayout:rowspan="3"
            />

    <FrameLayout
            android:id="@+id/simple_cell4"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            celllayout:cellX="2"
            celllayout:cellY="2"
            celllayout:colspan="2"
            celllayout:rowspan="2"
            />

    <FrameLayout
            android:id="@+id/simple_cell5"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            celllayout:cellX="2"
            celllayout:cellY="4"
            celllayout:colspan="2"
            celllayout:rowspan="2"
            />
</com.jlnshen.widget.celllayout.SimpleCellLayout>